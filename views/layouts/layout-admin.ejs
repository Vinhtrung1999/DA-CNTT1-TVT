                            </li>
                            <li class="<%= locals.page == 'addItems' ? 'active' : '' %>">
                                <a href="/admin/addItems">Add items</a>
                            </li>
                            <% } %>
                                <% if(locals.user.positionID==='MNG' ) {%>
                                    <li class="<%= locals.page == 'listStaffs' ? 'active' : '' %>">
                                        <a href="/admin/listStaffs">List staffs</a>
                                    </li>
                                    <li class="<%= locals.page == 'addStaff' ? 'active' : '' %>">
                                        <a href="/admin/addStaff">Add staff</a>
                                    </li>
                                    <% } %>
                                        <li class="<%= locals.page == 'profile' ? 'active' : '' %>">
                                            <a href="/admin/profile">Profile</a>
                                        </li>
                                        <li>
                                            <a href="/admin/logout">Logout</a>
                                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div> <!-- End mainmenu area -->
    <%- include('../adminPages/'+locals.page) -%>
        <div class="footer-top-area">
            <div class="zigzag-bottom"></div>
            <div class="container">
                <div class="row">
                    <div class="col-sm-6">
                        <div class="footer-about-us">
                            <img src="/img/header.png" alt="" style="width: 30%; height: 30%;">

                            <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Perferendis sunt id doloribus
                                vero
